{
	"name": "typescript-boilerplate",
	"version": "1.0.0",
	"description": "boilerplate for web applications using typescript and webpack",
	"main": "index.js",
	"scripts": {
		"start": "webpack serve",
		"build": "rm -rf dist && NODE_ENV='production' webpack",
		"deploy": "git add -f dist && git commit -m 'deploy' && git push origin `git subtree split --prefix dist master`:gh-pages --force && git rm -r dist && git commit -m 'cleanup deploy'",
		"release": "npm run build && npm run deploy"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"css-loader": "^6.4.0",
		"data-type-ts": "0.0.7",
		"glamor": "^2.20.40",
		"lodash": "^4.17.20",
		"mini-css-extract-plugin": "^2.4.2",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"superstruct": "^0.15.3",
		"triple-database": "^0.1.3",
		"tuple-database": "^1.2.5"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.175",
		"@types/react": "^17.0.30",
		"@types/react-dom": "^17.0.0",
		"@types/webpack": "^5.28.0",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.4.0",
		"ts-loader": "^9.2.6",
		"ts-node": "^10.3.0",
		"typescript": "^4.4.4",
		"webpack": "^5.58.2",
		"webpack-cli": "^4.9.0",
		"webpack-dev-server": "^4.3.1"
	}
}
